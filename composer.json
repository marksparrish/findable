{
    "name": "pipcommunications/findable",
    "description": "Laravel package for using Elasticsearch as a query engine for Eloquent-like models.",
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Parrish",
            "email": "mark@pipcommunications.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "elasticsearch/elasticsearch": "^8.0"
    },
    "require-dev": {
        "laravel/framework": "^10.0",
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^8.0",
        "orchestra/testbench-dusk": "^8.0",
        "laravel/pint": "^1.24"
    },
    "conflict": {
        "illuminate/support": "<10.0"
    },
    "autoload": {
        "psr-4": {
            "Findable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Findable\\FindableServiceProvider"
            ],
            "aliases": {
                "Findable": "Findable\\Findable"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "keywords": ["laravel", "elasticsearch", "search", "eloquent", "findable"],
    "homepage": "https://github.com/pipcommunications/findable",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
